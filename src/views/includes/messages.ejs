<% if (errors.length > 0) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-danger">
            <% errors.forEach(error => { %>
            <small><%= error %></small><br>
            <% }) %>
        </div>
    </div>
</div>
<% } %>

<% if (success.length > 0) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-success">
            <% success.forEach(error => { %>
            <small><%= success %></small><br>
            <% }) %>
        </div>
    </div>
</div>
<% } %>
    
